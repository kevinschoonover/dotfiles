{
  "coc.preferences.formatOnSaveFiletypes": [ "python", "typescript", "json", "html", "css", "html", "rust"],
  "python.formatting.provider": "black",
  "python.sortImports.path": "isort",
  "python.venvPath": "~/.cache/pypoetry/virtualenvs",
  "eslint.filetypes": ["typescript", "typescriptreact", "javascript", "react"],
  "eslint.autoFixOnSave": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
          "terraform",
          "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
